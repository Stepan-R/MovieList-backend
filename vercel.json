{
  "version": 2,
  "builds": [{ "src": "./src/index.js", "use":
  "@vercel/node"}],
  "routes": [{ 
    "src": "/(.*)" ,
    "dest": "/",
          "methods": ["GET","POST", "DELETE", "PATCH"],
          "headers": {
             "Access-Control-Allow-Origin": "*",
             "Access-Control-Allow-Credentials": "true"
         }
  }]
}
